


# .env - Working configuration template
# Copy this to your .env file and update the values

# Existing GitHub settings
GITHUB_TOKEN=
ALLOW_ORIGINS=["*"]
STATIC_DIR=static
USER_AGENT=Repo-Analyzer/1.0

# MongoDB Atlas URI - Leave empty for now, auth will work without it
MONGO_URI=
# JWT Settings - Generate a long random string for JWT_SECRET
JWT_SECRET=super_secret_key_change_this_to_something_very_long_and_random_for_production_use_123456789
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# Google OAuth Settings (optional - leave empty if not using Google sign-in)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
